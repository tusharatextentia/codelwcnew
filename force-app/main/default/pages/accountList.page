<apex:page standardController="Account" recordSetVar="accounts">
    <apex:pageBlock title="Accounts List">
    <apex:pageBlockTable value="{!accounts}" var="item">
        <apex:column value="{!item.Id}"/>
        <apex:column value="{!item.name}"/>
        <apex:column value="{!item.Sic}"/>
        <apex:column value="{!item.TickerSymbol}"/>
        <apex:column value="{!item.Website}"/>
        <apex:column value="{!item.NumberOfEmployees}"/>
        <apex:column value="{!item.AnnualRevenue}"/>
        <apex:column value="{!item.Phone}"/>
        <apex:column value="{!item.Type}"/>
    </apex:pageBlockTable>
    </apex:pageBlock>
</apex:page>